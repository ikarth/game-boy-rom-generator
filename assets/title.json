      "Revolution",
      "Wrath",
      "Quest",
      "War",
      "Voyage",
      "Conquest"
      ],
    "macguffin": [
      "#object.capitalize#",
      "#property.capitalize# #object.capitalize#",
      "#object.capitalize# of #place#",
      "#property.capitalize# #subject.capitalize#",
      "#subject.capitalize# of the #place#",
      "#event# of the #subject#"
    ],
    "word": ["#object#", "#attribute#", "#material#", "#story#", "#place#", "#subject#"],
    "iii": [
      "","","","","","","",
      " II", " II", " III", " IV", " II", " II", " III", " II","",""," II"," II"," V","",""," II"," II"," VI"],
    "title": ["#story# of the #macguffin#",
      "#story# for the #macguffin#",
      "#story# of #macguffin#",
      "#material.capitalize# #story.capitalize#",
      "#macguffin# #story.s#",
      "#material# #object#",
      "#word.capitalize##iii#: #word.capitalize# #word.capitalize#",
      "#property# #subject##iii#: #place#",
      "#subject#'s #title#",
      "#mat_or_attr# #word#",
      "#word# #word# #word#",
      "#macguffin#",
      "#word.s# & #word.s#",
      "#word# of #word.s#"
    ],
    "prefix": [
      "","Super "],
    "origin": [
      "#title#","#title#","#title#","#title#", "#prefix##title#"
    ]
  }
