      "Skull",
      "Dark",
      "Story",
      "Legendary",
      "Deep",
      "Ultimate",
      "Dragon",
      "Secret",
      "#attribute.capitalize#",
      "Sorceror",
      "#attribute.capitalize# Lord"
      ],
    "place": [
      "#place_thing#",
      "#place_thing# of the #place_attribute#",
      "#place_attribute# #place_thing#",
      "#place_thing.a# of #attribute.capitalize# et #attribute.capitalize#",
      "#place_thing# of the #subject#",
      "#place_thing# of the #mat_or_attr# #subject#"
    ],
    "story": [
      "Legend",
      "Quest",
      "Epic",
      "Saga",
      "Ballad",
      "Whisper",
